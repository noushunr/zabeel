<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:theme="@style/AppTheme.NoActionBar">

    <LinearLayout
        android:id="@+id/linear_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:gravity="bottom"
        android:orientation="vertical">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView_saveAddress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="-10dp"
            app:cardBackgroundColor="@color/toolbar_clr"
            app:cardCornerRadius="20dp"
            card_view:cardElevation="10dp"
            card_view:cardPreventCornerOverlap="false">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/linear_radiobutton"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:visibility="visible"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/address_billing_radioButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="20dp"
                        android:buttonTint="@color/green" />

                    <TextView
                        android:id="@+id/textView_radiobutton"
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:fontFamily="@font/helveticalight"
                        android:text="Make this as my default address"
                        android:textColor="@color/black"
                        android:textSize="15sp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_radiobutton1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/save_button"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:background="@drawable/box_green_border_green_background"
                        android:text="@string/submit_title_text"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="17sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:visibility="gone"
                    android:id="@+id/ll_guest">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_signIn"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:background="@drawable/box_green_border_green_background"
                            android:text="@string/sign_in"
                            android:textAllCaps="false"
                            android:textColor="@color/white"
                            android:textSize="17sp" />

                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/or"
                        android:gravity="center"
                        android:textSize="14sp"
                        android:fontFamily="@font/helveticamedium">

                    </TextView>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginRight="10dp"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_guest"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dp"
                            android:layout_marginRight="20dp"
                            android:backgroundTint="@color/background_light_green"
                            android:background="@drawable/box_green_border_green_background"
                            android:text="@string/zabeel_guest"
                            android:textAllCaps="false"
                            android:textColor="@color/white"
                            android:textSize="17sp" />

                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>



        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.google.android.material.appbar.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppTheme.NoActionBar"
            app:elevation="0dp">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar_addrress"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@color/toolbar_clr"
                android:theme="@style/AppTheme.NoActionBar">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    android:gravity="center">

                    <ImageButton
                        android:id="@+id/imageButton_address_back"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/ic_chevron_left_black_24dp" />

                    <TextView
                        android:id="@+id/addressText"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginEnd="60dp"
                        android:fontFamily="@font/helveticalight"
                        android:gravity="center"
                        android:padding="10dp"
                        android:text="@string/addresses_title_text"
                        android:textColor="@color/black"
                        android:textSize="17sp" />

                    <ImageButton
                        android:visibility="gone"
                        android:id="@+id/imageView_Address_cart"
                        android:layout_width="30dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="20dp"
                        android:layout_marginEnd="20dp"
                        android:background="@drawable/ic_action_cart" />

                </LinearLayout>

            </androidx.appcompat.widget.Toolbar>
        </com.google.android.material.appbar.AppBarLayout>
    </LinearLayout>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/linear_bottom"
        android:layout_marginStart="@dimen/margin_16"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="@dimen/margin_16"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_first_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:singleLine="true"
                    android:inputType="text"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/first_name_title_text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_last_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:singleLine="true"
                    android:inputType="text"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/last_name_title_text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_phone_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/mobile_number_title_text"
                    android:inputType="phone"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/email_input"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:visibility="gone"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/et_email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/email"
                    android:inputType="textEmailAddress"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_area_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:singleLine="true"
                    android:inputType="text"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/area_title_text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_block_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:singleLine="true"
                    android:inputType="text"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/block_title_text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:padding="@dimen/margin_8"
                android:text="@string/addresses_title_text"
                android:visibility="gone"
                android:textSize="16sp"
                android:textStyle="bold"/>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_street_address"
                    android:layout_width="match_parent"
                    android:layout_height="76dp"
                    android:background="@null"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/street_title_text"
                    android:textAlignment="textStart"
                    android:gravity="start"
                    android:inputType="textMultiLine"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_building_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:singleLine="true"
                    android:inputType="text"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/building_number_title_text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_floor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:singleLine="true"
                    android:inputType="text"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/floor_title_text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_apartment_number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:singleLine="true"
                    android:inputType="text"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/apartment_number_title_text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:fontFamily="@font/helveticalight"
                    android:hint="*City"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4"
                android:visibility="gone"
                tools:visibility="gone">

                <EditText
                    android:id="@+id/add_address_pincode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:fontFamily="@font/helveticalight"
                    android:hint="Zip/Postal code*"
                    android:inputType="number"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4"
                android:theme="@style/TextInputLayoutAppearances">

                <com.reginald.editspinner.EditSpinner
                    android:id="@+id/add_address_country"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:editable="false"
                    android:fontFamily="@font/helveticalight"
                    android:hint="*Country"
                    android:inputType="none"
                    android:padding="@dimen/margin_8"
                    app:dropDownDrawable="@drawable/spinner"
                    app:dropDownDrawableHeight="20dp"
                    app:dropDownDrawableSpacing="15dp"
                    app:dropDownDrawableWidth="20dp" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:visibility="gone"
                tools:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4"
                android:theme="@style/TextInputLayoutAppearances">

                <com.reginald.editspinner.EditSpinner
                    android:id="@+id/add_address_state"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:editable="false"
                    android:fontFamily="@font/helveticalight"
                    android:hint="State Province*"
                    android:inputType="none"
                    android:padding="@dimen/margin_8"
                    app:dropDownDrawable="@drawable/spinner"
                    app:dropDownDrawableHeight="20dp"
                    app:dropDownDrawableSpacing="15dp"
                    app:dropDownDrawableWidth="20dp" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_comment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/comment_title_text"
                    android:textAlignment="textStart"
                    android:gravity="start"
                    android:singleLine="true"
                    android:inputType="text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_16"
                android:layout_marginBottom="@dimen/margin_16"
                android:background="@drawable/edittext_background"
                android:padding="@dimen/margin_4">

                <EditText
                    android:id="@+id/add_address_company_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@null"
                    android:fontFamily="@font/helveticalight"
                    android:hint="@string/company_title_text"
                    android:singleLine="true"
                    android:inputType="text"
                    android:padding="@dimen/margin_8" />

            </com.google.android.material.textfield.TextInputLayout>

        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

